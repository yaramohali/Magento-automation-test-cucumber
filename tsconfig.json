{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": [
      "node",
      "webdriverio/async",
      "@wdio/cucumber-framework"
    ],
    "skipLibCheck": true
  }
}
